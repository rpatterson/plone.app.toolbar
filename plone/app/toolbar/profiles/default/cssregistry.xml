<?xml version="1.0"?>
<object name="portal_css">

    <!-- assign bundle to other scritps, for now -->
    <javascript bundle="tinymce" id="++resource++tinymce.stylesheets/tinymce.css" />

    <!-- Hiding portal messages and edit bar -->
    <stylesheet
        cacheable="False" compression="safe" cookable="True"
        enabled="True" bundle="default"
        id="++resource++plone.app.toolbar/src/plone_bbb.css"
        />

    <!-- customized bootstrap for plone -->
    <stylesheet
        cacheable="False" compression="none" cookable="True"
        enabled="True" bundle="toolbar"
        id="++resource++plone.app.toolbar/src/plone.min.css"
        />

    <!-- toolbar specific CSS -->
    <stylesheet
        cacheable="False" compression="none" cookable="True"
        enabled="True" bundle="toolbar"
        id="++resource++plone.app.toolbar/src/toolbar-plone.css"
        />

    <!-- BBB cleanup from old toolbar code -->
    <stylesheet
        remove="True"
        id="++resource++plone.app.toolbar/src/bootstrap-plone.min.css" />

</object>
